@page
@model DBRadio.Pages.Zf.Filtri.OtdelkadrovF
@{
}

<h1>Отдел кадров</h1>

<table class="table">
	<thead>
		<tr>
			<th>
				@Html.DisplayNameFor(model => model.Sotrudniki[0].FullName)
			</th>
			<th>
				@Html.DisplayNameFor(model => model.Sotrudniki[0].Position_ID)
			</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		@foreach (var item in Model.Sotrudniki)
		{
			<tr>
				<td>
					@Html.DisplayFor(modelItem => item.FullName)
				</td>
				<td>
						@Model.Dolzhnosti.JobTitle
				</td>
			</tr>
		}
	</tbody>
</table>
